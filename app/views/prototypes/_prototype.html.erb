<div class="card">
  <%= link_to image_tag(p.image, class: :card__img ), root_path%>
  <div class="card__body">
    <%= link_to "#{p.title}", prototype_path(p.id),class: :card__title%>
    <p class="card__summary">
      <%= "#{p.catch_copy}"%>
    </p>
    <%= link_to "#{p.user.name}", user_path(p.user.id), class: :card__user %>
  </div>
</div>